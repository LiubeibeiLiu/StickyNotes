<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:StickyNotes">
    <SolidColorBrush x:Key="ForgroundBrush" Color="{StaticResource ForgroundColorGray}" />
    <SolidColorBrush x:Key="ForgroundBrush100" Color="{StaticResource ForgroundColorGray100}" />
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColorGray}" />
</ResourceDictionary>